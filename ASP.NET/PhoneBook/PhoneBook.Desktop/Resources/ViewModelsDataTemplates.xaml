<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:vm="clr-namespace:PhoneBook.Desktop.ViewModels"
                    xmlns:view="clr-namespace:PhoneBook.Desktop.Views">

    <DataTemplate DataType="{x:Type vm:AnonymMainViewModel}">
        <view:AnonymMainView DataContext="{Binding}"/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:AdminMainViewModel}">
        <view:AdminMainView DataContext="{Binding}" />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:UserMainViewModel}">
        <view:UserMainView DataContext="{Binding}" />
    </DataTemplate>
</ResourceDictionary>